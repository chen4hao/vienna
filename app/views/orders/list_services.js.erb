<!-- 將 _listservicesmodal.html.erb 的 資料塞入 我們先前就建立好的 #listservicesmodal div -->
$('#listServicesModal').html("<%= escape_javascript(render 'listservicesmodal') %>");
<!-- 讓#listservicesmodal 呈現出來 -->
