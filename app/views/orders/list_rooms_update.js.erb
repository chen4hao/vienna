<!-- 將 _listroomsmodal.html.erb 的 資料塞入 我們先前就建立好的 #listRoomsModal div -->
$('#listRoomsModalUpdate').html("<%= escape_javascript(render partial: 'listroomsmodalupdate', locals: { order: @order, back_path: @back_path } ) %>");
<!-- 讓#listRoomsModal 呈現出來 -->
$("#listRoomsModalUpdate").modal("show");

<!-- Turn off submit on "Enter" key -->
$("form").on("keypress", function (e) {
  if (e.keyCode == 13) {
    return false;
  }
});
