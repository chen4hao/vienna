<%= simple_form_for [:admin, service] do |f| %>
  <div class="form-service">
    <%= f.input :name, required: true, input_html: { class: "form-control"}, autofocus: true %>
    <%= f.input :list_price, required: true, input_html: { class: "form-control"} %>
    <%= f.input :category, input_html: { class: "form-control"} %>

  </div>
  <%= f.button :submit, class: "btn btn-primary" %>
<% end %>
